<header class="container">
  <div class="centered">
    <img class="avatar" src="{{ gravatar contact.email }}">
  </div>

  <div class="two-columns">
    <div id="name-contacts-wrap" class="right-column">
      <h1 id="name">{{ name }}</h1>
      <div id="contacts">
        <p>
          <i class="emoji emoji-github"></i>
          <a class="contact" href="http://github.com/{{ contact.github }}">github.com/{{ contact.github }}</a>
        </p>
        <p>
          <i class="emoji emoji-email"></i>
          <a class="contact" href="{{ contact.email }}">{{ contact.email }}</a>
        </p>
        <p>
          <i class="emoji emoji-cell"></i>
          {{ contact.cell }}
        </p>
        <p>
          <i class="emoji emoji-location"></i>
          {{ contact.location }}
        </p>
      </div>
    </div>
  </div>
</header>

<div class="container">
  <div class="two-columns">
    <div class="left-column">
      <h2>Work Experience</h2>
    </div>
    <div class="right-column border-left">
      {{#each experience }}
        <div class="experience">
          <p class="time-line">
            <span class="experience-period-start">{{{ period.start }}}</span>
            &middot;&middot;&middot;
            <span class="experience-period-end">{{{ period.end }}}</span>
          </p>
          <h3><a href="{{ employer.href }}">{{{ employer.name }}}</a>, {{{ title }}}</h3>
          <ul>
            {{#each items }}
            <li>{{{ this }}}</li>
            {{/each}}
          </ul>
        </div>
      {{/each}}
    </div>
  </div>

  <div class="two-columns">
    <div class="left-column">
      <h2>Skills</h2>
      <ul id="language-graph">
        {{#each skills.languages }}
        <li>
          <span class="language-name">{{ name }}</span> <span class="language-years">({{ years }} years)</span>
        </li>
        {{/each}}
      </ul>
    </div>
    <div class="right-column border-left">
      <ul>
        {{#each skills.items}}
        <li>{{{ this }}}</li>
        {{/each}}
    </div>
  </div>

  <div class="two-columns">
    <div class="left-column">
      <h2>Education</h2>
    </div>
    <div class="right-column border-left">
      <p class="time-line">{{{ education.year }}}</p>
      <h3><a href="{{ education.university.href }}">{{{ education.university.name }}}</a></h3>
      <ul>
        <li>{{{ education.degree }}}</li>
        <li>Thesis: <a href="{{ education.thesis.href }}">{{{ education.thesis.title }}}</a></li>
      </ul>
    </div>
  </div>

